{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Linked Data Import output result DTO",
  "properties": {
    "resourcesWithLineNumbers": {
      "description": "Array of mapped resources with their line numbers",
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "$ref": "resourceWithLineNumber.json"
      }
    },
    "jobExecutionId": {
      "description": "Import Job Execution ID",
      "type": "integer",
      "format": "int64"
    },
    "tenant": {
      "description": "Tenant ID",
      "type": "string"
    },
    "ts": {
      "description": "Message timestamp",
      "type": "string"
    }
  },
  "x-implements": "org.folio.spring.tools.kafka.BaseKafkaMessage",
  "required": ["resourcesWithLineNumbers"]
}
